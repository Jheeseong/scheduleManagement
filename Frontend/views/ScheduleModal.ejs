<div class="modal_schedule">
    <div class="modal_schedule_body">
        <h2>일정 생성 및 편집</h2>
        <label>일정 :</label>
        <input type="datetime-local" id="startDate" clsss="startDate">
        <input type="datetime-local" id="endDate" clsss="endDate">
        <br/>
        <label>일정 제목 :</label>
        <input type="text" id="scheduleName" class="scheduleName">
        <br/>
        <label>일정 내용 :</label>
        <input type="text" id="scheduleContent" class="scheduleContent">
        <br/>
        <label>우선순위 :</label>
        <input type="range" id="schedulePriority" class="schedulePriority">
        <br/>
        <label>태그 :</label>
        <input type="input" id="scheduleTag" class="scheduleTag">
        <div id="map" style="width:100%;height:350px;"></div>
        <script type="text/javascript" src="//dapi.kakao.com/v2/maps/sdk.js?appkey=aae0c75d4e6a2c4fa1888b183f6100c9"></script>
        <script>
            var container = document.getElementById('map'); //지도를 담을 영역의 DOM 레퍼런스
            var options = { //지도를 생성할 때 필요한 기본 옵션
                center: new kakao.maps.LatLng(33.450701, 126.570667), //지도의 중심좌표.
                level: 3 //지도의 레벨(확대, 축소 정도)
            };

            var map = new kakao.maps.Map(container, options); //지도 생성 및 객체 리턴
        </script>
    </div>
</div>